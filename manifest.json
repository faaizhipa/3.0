{
   "action": {
      "default_icon": {
         "128": "icons/ExtLogoV3.png",
         "16": "icons/ExtLogoV3.png",
         "48": "icons/ExtLogoV3.png"
      },
      "default_popup": "popup.html",
      "default_title": "Salesforce Power-Up Extension"
   },
   "background": {
      "service_worker": "background.js"
   },
   "content_scripts": [ {
      "js": [ "content_script.js" ],
      "matches": [ "https://*.lightning.force.com/*", "https://*.salesforce.com/*", "https://wiki.clarivate.io/*" ],
      "run_at": "document_idle"
   } ],
   "description": "A multifaceted Chrome extension that deeply integrates into the Salesforce Lightning UI to act as an intelligent assistant for case management.",
   "icons": {
      "128": "icons/ExtLogoV3.png",
      "16": "icons/ExtLogoV3.png",
      "48": "icons/ExtLogoV3.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "Salesforce Power-Up Extension",
   "permissions": [ "storage", "tabs", "scripting" ],
   "host_permissions": [
      "https://*.lightning.force.com/",
      "https://*.salesforce.com/",
      "https://wiki.clarivate.io/"
    ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "3.0"
}
